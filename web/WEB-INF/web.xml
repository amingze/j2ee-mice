<?xml version="1.0" encoding="UTF-8"?>
<web-app>

    

    <servlet>
        <servlet-name>ForeServlet</servlet-name>
        <servlet-class>mice.servlet.ForeServlet</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>ForeServlet</servlet-name>
        <url-pattern>/foreServlet</url-pattern>
    </servlet-mapping>  



    <servlet>
        <servlet-name>ProductServlet</servlet-name>
        <servlet-class>mice.servlet.ProductServlet</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>ProductServlet</servlet-name>
        <url-pattern>/productServlet</url-pattern>
    </servlet-mapping>  

    <filter>
        <filter-name>JumpServletFilter</filter-name>
        <filter-class>mice.filter.JumpServletFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>JumpServletFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>ForeServletFilter</filter-name>
        <filter-class>mice.filter.ForeServletFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>ForeServletFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>EncodeServletFilter</filter-name>
        <filter-class>mice.filter.EncodeServletFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>EncodeServletFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
