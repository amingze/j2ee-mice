<?xml version="1.0" encoding="UTF-8"?>
<web-app>

    

    
    <servlet>
        <servlet-name>GoodsServlet</servlet-name>
        <servlet-class>mice.servlet.GoodsServlet</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>GoodsServlet</servlet-name>
        <url-pattern>/goodsServlet</url-pattern>
    </servlet-mapping>  

    <filter>
        <filter-name>JumpServletFilter</filter-name>
        <filter-class>mice.filter.JumpServletFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>JumpServletFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
